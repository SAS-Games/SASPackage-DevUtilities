  m_SubCommands:
  - Name: LogLevel
    HelpText: 'Usage: LogLevel <Level> <On|Off> \nEnables or disables a specific
      log level.  \nAvailable levels: Info, Warning, Error. \nExample: Logging.LogLevel
      Info On'
    Presets:
    - LogLevel Info On
    - LogLevel Warning On
    - LogLevel Error On
    - LogLevel Info Off
    - LogLevel Warning Off
    - LogLevel Error Off
    MethodName: LogLevel
  - Name: OnScreenLog
    HelpText: 'Usage: ShowOnScreen <On|Off> \n Toggles the on-screen logging display.
      \nExample: Logging.ShowOnScreen O'
    Presets:
    - OnScreenLog On
    - OnScreenLog Off
    MethodName: ShowOnScreen
  - Name: SetTags
    HelpText: 'Usage: SetTags <tag1|tag2|...> \nSets the allowed log tags to filter
      console output.\nProvide one or more tags separated by the pipe symbol ''|''.
      Only logs with these tags will be shown. \nExample: Logging.setTags Gameplay|UI|Network'
    Presets: []
    MethodName: SetTags
  - Name: ClearTags
    HelpText: 'ClearTags \nRemove all the Tags and prints all logs. \nExample: Logging.ClearTags'
    Presets:
    - ClearTags
    MethodName: ClearTags
  - Name: SetLogLifetime
    HelpText: 'SetLogLifetime <val> \nSets the lifetime of a log entry. \nExample:
      Logging.SetLogLifetime 5'
    Presets:
    - SetLogLifetime 2
    - SetLogLifetime 5
    MethodName: SetLogLifetime
  - Name: ClearOnScreenLog
